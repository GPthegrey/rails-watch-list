<%= simple_form_for [@list, Review.new], data: {
          insert_in_list_target: "form",
          action: "submit->insert-in-list#send",
          url: "/lists/#{@list.id}/reviews",
          method: "post"
        } do |f| %>
          <%= f.input :comment %>
          <%= f.input :rating %>
          <%= f.submit %>
        <% end %>
